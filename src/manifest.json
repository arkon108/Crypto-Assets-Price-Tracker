{
  "name": "Crypto Assets Price Tracker",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Quickly check the state of your crypto assets in chosen currency",
  "homepage_url": "https://github.com/arkon108/Crypto-Assets-Price-Tracker",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "service_worker": "service_worker.js"
  },
  "options_page": "options.html",
  "action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Crypto Assets Price Tracker",
    "default_popup": "browser_action.html"
  },
  "permissions": [
    "storage"
  ]
}